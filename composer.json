{
    "name": "dvelum/orm-ui",
    "version": "4.0.0",
    "type": "project",
    "description": "UI Form DVelum ORM",
    "keywords": [
        "dvelum",
        "php",
        "ui",
        "orm"
    ],
    "homepage": "https://github.com/dvelum/orm-ui",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Kirill Yegorov",
            "role": "Founder, Developer"
        },
        {
            "name": "Andrew Zamotaev",
            "role": "Developer"
        },
        {
            "name": "Sergey Leschenko",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/dvelum/orm-ui/issues",
        "source": "https://github.com/dvelum/orm-ui"
    },
    "config": {
        "preferred-install": "dist",
        "classmap-authoritative": true
    },
    "require": {
        "php": ">=7.4.0",
        "ext-mysqli": "*",
        "ext-mbstring": "*",
        "ext-json": "*",
        "ext-fileinfo": "*",
        "composer/installers": "~1.9",
        "oomphinc/composer-installers-extender": "^2.0",
        "dvelum/extjs": "^7.0.0",
        "dvelum/ext-library": "^1.0.0",
        "nette/neon": "^3.2",
        "matthiasmullie/minify": "~1.3"
    },
    "extra": {
        "installer-types": [
            "js-library"
        ],
        "installer-paths": {
            "public/js/lib/{$name}": [
                "type:js-library"
            ]
        }
    },
    "bin": [
        "orm-ui"
    ],
    "autoload": {
        "psr-4": {
            "Dvelum\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "phpstan/phpstan": "^0.12",
        "codacy/coverage": "dev-master"
    }
}
